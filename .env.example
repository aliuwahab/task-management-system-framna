###> symfony/framework-bundle ###
APP_ENV=dev
APP_SECRET=change-me-in-production
###< symfony/framework-bundle ###

###> doctrine/doctrine-bundle ###
# SQLite database (works out of the box - no setup needed!)
DATABASE_URL="sqlite:///%kernel.project_dir%/var/data.db"

# For other databases, uncomment and configure:
# DATABASE_URL="mysql://db_user:db_password@127.0.0.1:3306/db_name?serverVersion=8.0"
# DATABASE_URL="postgresql://db_user:db_password@127.0.0.1:5432/db_name?serverVersion=15&charset=utf8"
###< doctrine/doctrine-bundle ###

###> symfony/routing ###
# Default URI for console commands
DEFAULT_URI=http://localhost:8000
###< symfony/routing ###

###> nelmio/cors-bundle ###
# CORS configuration for API
CORS_ALLOW_ORIGIN='^https?://(localhost|127\.0\.0\.1)(:[0-9]+)?$'
###< nelmio/cors-bundle ###
